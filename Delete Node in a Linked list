/**
 * Definition for singly-linked list.
 * public class ListNode {
 *     int val;
 *     ListNode next;
 *     ListNode(int x) { val = x; }
 * }
 */
class Solution {
    
    public void deleteNode(ListNode ptr) {
         ListNode temp=ptr.next;
         ptr.val=temp.val;
         ptr.next=temp.next ;
 
         temp = null;       
        
}
}
